#%RAML 1.0 DataType

type: object



properties:
  id?: number
  title: string
  description: string
  lat?: number
  lng?: number
  startDate: string
  endDate: string
  hidden: boolean
  status: !include Status.raml
  category: !include Category.raml
  user: !include User.raml
  images?: 
    type: array
    items: !include Image.raml
